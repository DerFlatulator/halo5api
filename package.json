{
  "name": "haloapi",
  "version": "0.1.5",
  "description": "JavaScript wrapper of the Halo 5 Developer's API using TypeScript.",
  "main": "js/index.js",
  "typings": "js/haloapi",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/mocha",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "clean": "rm js/*.js",
    "document": "typedoc --out docs ./ts/index.ts --module commonjs --includeDeclarations"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DerFlatulator/haloapi.git"
  },
  "keywords": [
    "halo 5",
    "api"
  ],
  "author": "Der Flatulator",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DerFlatulator/halo5api/issues"
  },
  "homepage": "https://github.com/DerFlatulator/haloapi",
  "dependencies": {
    "request-promise": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "chai-string": "^1.1.3",
    "mocha": "^2.3.3",
    "typescript": "^1.6.2"
  },
  "typescript": {
    "main": "./ts/index.ts"
  },
  "tonicExampleFilename": "examples/matches.js"
}
